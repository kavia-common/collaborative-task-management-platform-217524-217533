# Frontend environment variables for TaskBoards
# Replace values when deploying. Do not commit a real .env with secrets.

# Base URL of backend REST API (e.g., http://localhost:8000 or https://api.example.com)
REACT_APP_API_BASE_URL=http://localhost:8000

# WebSocket URL for realtime presence/updates (e.g., ws://localhost:8000/ws or wss://api.example.com/ws)
REACT_APP_WS_URL=ws://localhost:8000/ws

# Site URL used for auth providers (redirects/emails if applicable)
REACT_APP_SITE_URL=http://localhost:3000
