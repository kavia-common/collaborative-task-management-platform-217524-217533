# Frontend environment variables
# Base URL for backend API (e.g., http://localhost:3001)
REACT_APP_API_BASE_URL=http://localhost:3001
# Base WS URL for realtime (e.g., ws://localhost:3001)
REACT_APP_WS_URL=ws://localhost:3001
# Optional: Force demo mode without backend (true|false). When true, disables server mutations and uses in-memory mock data.
REACT_APP_DEMO_MODE=false
