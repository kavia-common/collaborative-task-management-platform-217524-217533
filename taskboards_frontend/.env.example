# Frontend environment example for TaskBoards

# Base URL of your backend API (e.g., http://localhost:3001)
REACT_APP_API_BASE_URL=

# Base WebSocket URL (e.g., ws://localhost:3001/ws)
REACT_APP_WS_URL=

# Enable demo mode to explore UI without backend (true/false)
REACT_APP_DEMO_MODE=false

# Optional flags used by Developer diagnostics modal
REACT_APP_NODE_ENV=development
REACT_APP_FEATURE_FLAGS=
REACT_APP_EXPERIMENTS_ENABLED=
REACT_APP_DATABASE_URL=
